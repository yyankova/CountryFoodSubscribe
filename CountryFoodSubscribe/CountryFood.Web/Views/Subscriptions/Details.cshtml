@model CountryFood.Web.ViewModels.SubscriptionViewModel
@{
    ViewBag.Title = Model.Title;
}

<h2>@Model.Title</h2>
@Html.DisplayFor(m => m.Product)
<br />
@Html.DisplayFor(m => m.Producer)
<br />
@Html.DisplayFor(m => m.State)
<br />
@Html.DisplayFor(m => m.PeriodStart)
<br />
@Html.DisplayFor(m => m.PeriodEnd)
<br />
@Html.DisplayFor(m => m.Frequency)
<br />
@Html.DisplayFor(m => m.Address)
<br />

@Html.ActionLink("Change Subscription", "Cancel", "Subscriptions", new { id = Model.Id}, new { @class = "btn btn-warning" })
@Html.ActionLink("Cancel Subscription", "Cancel", "Subscriptions", new { id = Model.Id }, new { @class = "btn btn-danger" })